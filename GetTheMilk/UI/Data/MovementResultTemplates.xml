<?xml version="1.0"?>
<ArrayOfMessagesFor xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <MessagesFor ResultType="Ok">
    <Messages>
      <Message Id="To Walk" Value="{0} {1} {3}." />
      <Message Id="To Run" Value="{0} {1} {3}." />
      <Message Id="To Enter" Value="{0} {1} level {4}." />
    </Messages>
  </MessagesFor>
  <MessagesFor ResultType="OriginNotOnTheMap">
    <Messages>
      <Message Id="To Walk" Value="Error {0} cannot {2}." />
      <Message Id="To Run" Value="Error {0} cannot {2}." />
    </Messages>
  </MessagesFor>
  <MessagesFor ResultType="OutOfTheMap">
    <Messages>
      <Message Id="To Walk" Value="{0} tried to {2} {3} but the world has limits." />
      <Message Id="To Run" Value="{0} tried to {2} {3} but the world has limits." />
    </Messages>
  </MessagesFor>
  <MessagesFor ResultType="BlockedByObject">
    <Messages>
      <Message Id="To Walk" Value="{0} tried to {2} {3} but is impossible, blocked by a {5}." />
      <Message Id="To Run" Value="{0} tried to {2} {3} but is impossible, blocked by a {5}." />
      <Message Id="To Enter" Value="{0} tried to {2} level {4} but is impossible, blocked by a {5}." />
    </Messages>
  </MessagesFor>
  <MessagesFor ResultType="BlockedByCharacter">
    <Messages>
      <Message Id="To Walk" Value="{0}  tried to {2} {3} but {6} is on the way." />
      <Message Id="To Run" Value="{0}  tried to {2} {3} but {6} is on the way." />
      <Message Id="To Enter" Value="{0}  tried to {2} level {4} but {6} is on the way." />
    </Messages>
  </MessagesFor>
</ArrayOfMessagesFor>