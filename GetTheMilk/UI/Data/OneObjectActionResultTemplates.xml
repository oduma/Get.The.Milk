<?xml version="1.0"?>
<ArrayOfMessagesFor xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <!--{0} Player.Name.Narrator
      {1} Action.Name.Past
      {2} Action.Name.Present
      {3} TargetObject.Name.Narrator-->
  <MessagesFor ResultType="Ok">
    <Messages>
      <Message Id="To Pick" Value="{0} {1} {3}." />
      <Message Id="To Kick" Value="{0} {1} {3}." />
      <Message Id="To Keep" Value="{0} decided to {2} {3}." />
    </Messages>
  </MessagesFor>
  <MessagesFor ResultType="NotOk">
    <Messages>
      <Message Id="To Pick" Value="{0} tried to {2} {3} but couldn't." />
      <Message Id="To Kick" Value="{0} tried to {2} {3} but couldn't." />
      <Message Id="To Keep" Value="{0} tried to {2} {3} but couldn't." />
    </Messages>
  </MessagesFor>
</ArrayOfMessagesFor>